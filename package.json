{
  "name": "phaser3-autotile",
  "version": "0.2.4",
  "description": "Phaser 3 Autotile",
  "main": "src/index.js",
  "watch": {
    "build:exampleblobs": {
      "patterns": [
        "examples/src/raw"
      ],
      "extensions": [
        "png"
      ]
    }
  },
  "bin": "./tools/blob.mjs",
  "scripts": {
    "build": "webpack",
    "build:exampleblobs": "phaser3-autotile -i ./examples/src/raw/*.png -o ./examples/src/cooked/",
    "examples": "webpack-dev-server -d --context examples --config examples/webpack.config.js --open",
    "test": "jest",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:clear": "jest --clearCache",
    "watch": "npm-watch",
    "lint": "eslint --fix --ext .js,.jsx,.mjs ."
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/browndragon/phaser3-autotile.git"
  },
  "author": "Brown Dragon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/browndragon/phaser3-autotile/issues"
  },
  "homepage": "https://github.com/browndragon/phaser3-autotile#readme",
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@webpack-cli/init": "^0.3.0",
    "acorn-jsx": "^5.2.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.1.0",
    "csv-loader": "^3.0.3",
    "directory-tree": "^2.2.4",
    "eslint": "^7.9.0",
    "eslint-plugin-jest": "^23.17.1",
    "file-loader": "^4.2.0",
    "fs": "0.0.1-security",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^26.4.2",
    "jest-canvas-mock": "^2.2.0",
    "jimp": "^0.16.1",
    "mkdirp": "^1.0.4",
    "npm-watch": "^0.7.0",
    "phaser": "^3.x",
    "raw-loader": "^3.1.0",
    "terser-webpack-plugin": "^2.2.1",
    "typescript": "^3.9.6",
    "webpack": "^4.43.0",
    "webpack-cli": "~3.0.0",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2",
    "xml-loader": "^1.2.1"
  },
  "dependencies": {
    "seedrandom": "^3.0.5"
  },
  "peerDependencies": {
    "phaser": "^3.x"
  },
  "jest": {
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "moduleDirectories": [
      "src",
      "tools",
      "__mocks__",
      "node_modules"
    ],
    "transform": {
      "^.+\\.(js|mjs)$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "mjs"
    ]
  }
}
